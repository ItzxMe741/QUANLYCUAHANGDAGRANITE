@model IEnumerable<CUAHANGBANDAGRANITE.Models.Quycachdonggoi>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Maquycach)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tenquycach)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dientichbemat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Khoiluong)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sovien)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ghichu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Active)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Maquycach)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tenquycach)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dientichbemat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Khoiluong)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sovien)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ghichu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Active)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idquycach">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idquycach">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idquycach">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
